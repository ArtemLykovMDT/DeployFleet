<calculatedSensorServiceConfigSection>
  <calculatedSensorConfigs>
    <!-- Control Override for all blenders. Do not change. Use another calculated sensor is a blender needs to do something for control like eblender -->
    <calculatedSensorConfigItem calculationType="Expression" ignoreConnectionStatus="true" sensorKey="CALC:HAS_CONTROL" outputUnitOfMeasure="RAW" expression="1"></calculatedSensorConfigItem>

    <!-- Works with LA08 and Eblender. If another Blender needs remote control, use these sensor names SW_IN_REMOTE_CONTROL or REMOTE_CONTROL -->
    <calculatedSensorConfigItem calculationType="Expression"
      ignoreConnectionStatus="true"
      sensorKey="CALC:HAS_BLENDER_CONTROL"
      outputUnitOfMeasure="RAW"
      expression="if(isNc(Rc1), if(isNc(Rc2),1,if(Rc2,1,0)),if(Rc1, 1, 0))">
      <inputs>
        <expressionSensorInput expressionIdentifier="Rc1" name="Rc1" sensorKey="BLENDER1:SW_IN_REMOTE_CONTROL" unitOfMeasure="BOOL" />
        <expressionSensorInput expressionIdentifier="Rc2" name="Rc2" sensorKey="BLENDER1:REMOTE_CONTROL" unitOfMeasure="BOOL" />
      </inputs>
    </calculatedSensorConfigItem>
    <calculatedSensorConfigItem calculationType="Expression"
       ignoreConnectionStatus="true"
       sensorKey="CALC:BLENDER2_HAS_CONTROL"
       outputUnitOfMeasure="RAW"
       expression="if(isNc(Rc1), if(isNc(Rc2),1,if(Rc2,1,0)),if(Rc1, 1, 0))">
      <inputs>
        <expressionSensorInput expressionIdentifier="Rc1" name="Rc1" sensorKey="BLENDER2:SW_IN_REMOTE_CONTROL" unitOfMeasure="BOOL" />
        <expressionSensorInput expressionIdentifier="Rc2" name="Rc2" sensorKey="BLENDER2:REMOTE_CONTROL" unitOfMeasure="BOOL" />
      </inputs>
    </calculatedSensorConfigItem>
    <calculatedSensorConfigItem calculationType="Expression"
      ignoreConnectionStatus="true"
      sensorKey="CALC:BLENDER1_COMMON_STATUS"
      outputUnitOfMeasure="RAW"
      expression="if(not(isNc(ekill)) and ekill, -3, if(hasControl and runIdle == 0, -2, 1))">
      <inputs>
        <expressionSensorInput expressionIdentifier="hasControl" name="hasControl" sensorKey="CALC:HAS_BLENDER_CONTROL" unitOfMeasure="BOOL" />
        <expressionSensorInput expressionIdentifier="runIdle" name="runIdle" sensorKey="BLENDER1:SW_RUN_IDLE" unitOfMeasure="BOOL" />
        <expressionSensorInput expressionIdentifier="ekill" name="ekill" sensorKey="BLENDER1:BLENDER_TUB.IN_EKILL" unitOfMeasure="BOOL" />
      </inputs>
    </calculatedSensorConfigItem>
    <calculatedSensorConfigItem calculationType="Expression"
      ignoreConnectionStatus="true"
      sensorKey="CALC:BLENDER2_COMMON_STATUS"
      outputUnitOfMeasure="RAW"
      expression="if(not(isNc(ekill)) and ekill, -3, if(hasControl and runIdle == 0, -2, 1))">
      <inputs>
        <expressionSensorInput expressionIdentifier="hasControl" name="hasControl" sensorKey="CALC:BLENDER2_HAS_CONTROL" unitOfMeasure="BOOL" />
        <expressionSensorInput expressionIdentifier="runIdle" name="runIdle" sensorKey="BLENDER2:SW_RUN_IDLE" unitOfMeasure="BOOL" />
        <expressionSensorInput expressionIdentifier="ekill" name="ekill" sensorKey="BLENDER2:BLENDER_TUB.IN_EKILL" unitOfMeasure="BOOL" />
      </inputs>
    </calculatedSensorConfigItem>
    <!-- This is a Sensor to mock remote control for sand in blenders since other than eBlender none of  -->
    <calculatedSensorConfigItem calculationType="Expression"
      ignoreConnectionStatus="true"
      sensorKey="CALC:MANUAL_SAND1_HAS_CONTROL"
      outputUnitOfMeasure="BOOL"
      expression="if(isNc(InRemoteControl), 1, if(InRemoteControl, 1, 0))">
      <inputs>
        <expressionSensorInput expressionIdentifier="InRemoteControl" name="InRemoteControl" sensorKey="BLENDER1:REMOTE_CONTROL" unitOfMeasure="BOOL" />
      </inputs>
    </calculatedSensorConfigItem>
    <calculatedSensorConfigItem calculationType="Expression"
      ignoreConnectionStatus="true"
      sensorKey="CALC:MANUAL_SAND2_HAS_CONTROL"
      outputUnitOfMeasure="BOOL"
      expression="1">
    </calculatedSensorConfigItem>
